configuration {
  icon-theme: "Papirus-Light";
  show-icons: true;
	font: "JetBrains Mono 12";
	location: 0;
	yoffset: 0;
  hover-select: true;
  timeout {
      action: "kb-cancel";
      delay:  20;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}
@theme "theme"
